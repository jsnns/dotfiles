{
    "workbench.iconTheme": "vscode-icons",
    "explorer.autoReveal": true,
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "editor.formatOnSave": true,
    "workbench.sideBar.location": "left",
    "workbench.editor.labelFormat": "short",
    "git.autofetch": true,
    "git.autoStash": true,
    "git.enableCommitSigning": true,
    "git.fetchOnPull": true,
    "git.enableSmartCommit": true,
    "rust-client.enableMultiProjectSetup": true,
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "terminal.integrated.fontFamily": "'Fira Code'",
    "git.confirmSync": false,
    "editor.codeActionsOnSave": {
		"source.organizeImports": true
	},
    "vim.easymotion": true,
    "vim.sneak": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "d"
            ],
            "after": [
                "d",
                "d"
            ]
        },
        {
            "before": [
                "<C-n>"
            ],
            "commands": [
                ":nohl"
            ]
        }
    ],
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false
    },
    "vscode_custom_css.imports": [
        "file:///Users/jacobsansbury/.me/vscode.css"
    ],
    "typescript.updateImportsOnFileMove.enabled": "always",
    "editor.multiCursorModifier": "ctrlCmd",
    "latex-workshop.view.pdf.viewer": "tab",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "python.defaultInterpreterPath": "/Users/jacobsansbury/.local/share/virtualenvs/jsnns-4LXk0B_e/bin/python",
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "javascript.updateImportsOnFileMove.enabled": "always",
    "[markdown]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "python.languageServer": "Pylance",
    "editor.fontSize": 13,
    "rust.mode": null,
    "[python]": {
        "editor.defaultFormatter": "ms-python.python"
    },
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.inlineSuggest.enabled": true,
    "security.workspace.trust.untrustedFiles": "open",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "typescriptHero.imports.disableImportsSorting": true,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
}